# Source Account (to export from)
SOURCE_FRONTEGG_CLIENT_ID=your-source-client-id-here
SOURCE_FRONTEGG_SECRET_KEY=your-source-secret-key-here
SOURCE_FRONTEGG_REGION=US

# Destination Account (to import to)
DEST_FRONTEGG_CLIENT_ID=your-destination-client-id-here
DEST_FRONTEGG_SECRET_KEY=your-destination-secret-key-here  
DEST_FRONTEGG_REGION=US

# Environment Configuration (optional)
# Options: development, qa, staging, production
# Defaults to development if not specified
FRONTEGG_ENVIRONMENT=development